{
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@app/*": ["./src/app/*"],
      "@components/*": ["./src/app/(component)/*"],
      "@assets/*": ["./public/assets/*"],
      "@images/*": ["./public/assets/images/*"],
      "@icons/*": ["./public/assets/icons/*"],
      "@service/*": ["./src/service/*"]
    }
  }
}
